namespace Exercise3
{
    class E4
    {
        static void Main()
        {
            for (int j = 1; j < 1000; j++)
            {
                int n = 0;
                for (int i = 1; i < j; i++)
                {
                    if (j % i == 0)
                    {
                        n = n + i;
                    }
                }
                if (n == j)
                {
                    Console.WriteLine(n);
                }
            }

        }
    }
}
